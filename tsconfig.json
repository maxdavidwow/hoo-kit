{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"preserveConstEnums": true,
		"declaration": true,
		"suppressImplicitAnyIndexErrors": true,
		"outDir": "./dist"
	},
	"include": ["./src/**/*.d.ts", "./src/**/*.ts"]
}
