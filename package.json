{
	"name": "@wow-kit/hoo-kit",
	"author": "wow-kit",
	"version": "0.0.3",
	"description": "Hoo-Kit lets you hook onto many events to handle your processes",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"start": "tsc && node dist/index.js --customUiPath D:/dev/hoo-kit/default-ui/dist/default-ui",
		"run": "node dist/index.js --customUiPath D:/dev/hoo-kit/default-ui/dist/default-ui",
		"build": "tsc",
		"publish": "tsc --inlineSourceMap false && npm publish --access public"
	},
	"bin": {
		"hoo-kit": "./hoo-kit"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wow-kit/hoo-kit.git"
	},
	"license": "GPLv3",
	"dependencies": {
		"chokidar": "^3.4.2",
		"glob": "^7.1.6",
		"minimist": "^1.2.5",
		"uuid": "^8.3.0",
		"ws": "^7.3.1"
	},
	"devDependencies": {
		"@types/node": "^14.6.0",
		"@types/ws": "^7.2.6"
	}
}
